*{box-sizing:border-box}body,html{overflow-x:hidden;padding:0;margin:0;font-family:"Raleway",Calibri,Arial,sans-serif}img{max-width:100%;width:auto;height:auto;display:block}video{width:100%;max-width:100%;height:auto}h1,h2,h3,h4,h5,h6{font-family:"Dosis",Georgia,serif;line-height:100%}h1{font-size:4vw}footer{position:relative;z-index:1000;content-visibility:auto;contain-intrinsic-size:500px}.footer-divider{display:none}@media all and (max-width: 960px){.footer-divider{position:relative;z-index:999;display:block;height:130px;margin-top:-20px;margin-bottom:-110px}}header .hovering-gotobottom-btn{position:absolute;right:0;top:0;background-color:rgba(0,0,0,0);border:none;z-index:1000}header .maintenance-modal-container{position:fixed;left:0;right:0;top:0;bottom:0;z-index:2000;background-color:rgba(0,0,0,.4);pointer-events:none}header .maintenance-modal-container .maintenance-modal-content{background-color:#d2d4fd;max-width:500px;padding:1rem;margin:auto;margin-top:2rem;border-radius:.5rem;filter:drop-shadow(0 0 10px black)}section .center{padding:0px 15px;max-width:720px;margin:auto}.float-left,.float-right{max-width:360px}@media all and (max-width: 660px){.float-left,.float-right{max-width:100%;clear:both;float:none;text-align:center;width:100%}}.float-left{float:left;margin-right:20px}.float-right{float:right;margin-left:20px}.limit-width{max-width:720px;padding:15px;margin:auto}.horizontal-flex-container .flex{display:flex;flex-wrap:nowrap;align-items:center}.horizontal-flex-container .flex .text-side,.horizontal-flex-container .flex .non-text-side{width:100%;padding:15px}.horizontal-flex-container .flex .non-text-side video{width:100%}@media all and (max-width: 660px){.horizontal-flex-container .flex{flex-wrap:wrap}}.horizontal-flex-container.reversed .text-side{order:1}.horizontal-flex-container.reversed .non-text-side{order:0}.entry-banner{position:relative;margin-top:-2vw;height:29vw;content-visibility:auto;contain-intrinsic-size:1000px}.entry-banner .horizontal-flex{display:flex;flex-wrap:nowrap;z-index:100;align-items:center;position:absolute;left:0;right:0;top:0;bottom:0}.page-navigation{background-color:#fff}video{background-color:#fff}