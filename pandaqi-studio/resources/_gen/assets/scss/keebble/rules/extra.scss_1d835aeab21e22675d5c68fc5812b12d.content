@use "sass:map";@use "sass:math";@font-face{font-family:'Arbutus Slab';src:url(../../assets/ArbutusSlab-Regular.woff2);font-weight:normal;font-style:normal;font-display:swap}@page{size:A4;margin:0mm;margin-top:20mm;margin-bottom:20mm;@top-center{content:element(runningHeader)}@bottom-center{content:counter(page);font-style:italic}}.icon-start{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:0%;background-position-y:0%;margin:auto}.icon-double-word{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-100%;background-position-y:0%;margin:auto}.icon-triple-word{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-200%;background-position-y:0%;margin:auto}.icon-double-letter{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-300%;background-position-y:0%;margin:auto}.icon-triple-letter{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-400%;background-position-y:0%;margin:auto}.icon-big-ask{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-500%;background-position-y:0%;margin:auto}.icon-thief{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-600%;background-position-y:0%;margin:auto}.icon-destroyer{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-700%;background-position-y:0%;margin:auto}.icon-go-again{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:0%;background-position-y:-100%;margin:auto}.icon-garbage{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-100%;background-position-y:-100%;margin:auto}.icon-blockade{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-200%;background-position-y:-100%;margin:auto}.icon-collector{background-image:url(../../assets/special_cells.webp);background-size:800%;background-position-x:-300%;background-position-y:-100%;margin:auto}#running-header{position:running(runningHeader);background-color:black;display:block;width:100%;height:100%}#running-footer{position:running(runningFooter)}h1,h2,h3,h4,h5,h6{font-family:"Arbutus Slab",sans-serif}body{font-family:"Arbutus Slab",serif;background-color:#FCFCFC;font-size:17px}h1{font-size:3.5em}h2{font-size:2.0em;string-set:title content(text)}h3{font-size:1.0em}ul{margin-top:-0.85em;margin-bottom:1.5em}#setup{background-color:#d9ffa9}#setup .heading-container{transition:background-color 0.15s, box-shadow 0.15s}#setup .heading-container:hover{background-color:white;box-shadow:0 0 5px black}#objective{background-color:#68fed3}#objective .heading-container{transition:background-color 0.15s, box-shadow 0.15s}#objective .heading-container:hover{background-color:white;box-shadow:0 0 5px black}#expansions{background-color:#9c9ce9}#expansions .heading-container{transition:background-color 0.15s, box-shadow 0.15s}#expansions .heading-container:hover{background-color:white;box-shadow:0 0 5px black}#action_play{background-color:#ffb6df}#action_play .heading-container{transition:background-color 0.15s, box-shadow 0.15s}#action_play .heading-container:hover{background-color:white;box-shadow:0 0 5px black}#action_letters{background-color:#ffd68f}#action_letters .heading-container{transition:background-color 0.15s, box-shadow 0.15s}#action_letters .heading-container:hover{background-color:white;box-shadow:0 0 5px black}.rules-icon-scoring{filter:sepia(100%) saturate(500%) brightness(100%) hue-rotate(10deg) drop-shadow(0 0 2px #222)}.rules-icon-setup{filter:sepia(100%) saturate(500%) brightness(100%) hue-rotate(45deg) drop-shadow(0 0 2px #222)}.rules-icon-objective{filter:sepia(100%) saturate(500%) brightness(100%) hue-rotate(160deg) drop-shadow(0 0 2px #222)}.rules-icon-action{filter:sepia(100%) saturate(500%) brightness(100%) hue-rotate(270deg) drop-shadow(0 0 2px #222)}.rules-icon-expansion{filter:sepia(100%) saturate(500%) brightness(100%) hue-rotate(180deg) drop-shadow(0 0 2px #222)}@media print{body{font-size:14px}#setup{background-color:transparent !important}#setup .heading-container{background-color:#d9ffa9}#objective{background-color:transparent !important}#objective .heading-container{background-color:#68fed3}#expansions{background-color:transparent !important}#expansions .heading-container{background-color:#9c9ce9}#action_play{background-color:transparent !important}#action_play .heading-container{background-color:#ffb6df}#action_letters{background-color:transparent !important}#action_letters .heading-container{background-color:#ffd68f}}
