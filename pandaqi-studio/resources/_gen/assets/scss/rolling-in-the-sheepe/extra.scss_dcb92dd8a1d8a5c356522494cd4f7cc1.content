body{font-family:'Skranji', serif;font-size:18px;line-height:140%;background-color:#e4fba3;color:#101010}h1,h2,h3,h4,h5,h6{font-family:'Skranji', cursive}.image-as-heading{max-width:100%;margin-top:60px;margin-bottom:-20px;text-align:center}.image-as-heading img{width:100%;max-width:900px;margin:auto}.buy-link-container .buy-link{font-family:"Skranji";font-size:36px;border-radius:0px;filter:none;box-shadow:none;border-radius:5px;background-color:#385c17;color:#e1ffc5;transition:color 0.3s, filter 0.3s, transform 0.3s}.buy-link-container .buy-link:hover{background-color:#d3ffaa;color:#385c17;filter:drop-shadow(0 0 5px #000);transform:scale(1.1)}a{color:#385c17;transition:color 0.3s}a:hover{color:#4b7523}

