@font-face{font-family:griffy;src:url(fonts/Griffy-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:'"barlow condensed"';src:url(fonts/BarlowCondensed-Italic.woff2)format("woff2");font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:'"barlow condensed"';src:url(fonts/BarlowCondensed-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:'"barlow condensed"';src:url(fonts/BarlowCondensed-Black.woff2)format("woff2");font-weight:900;font-style:normal;font-display:swap}body{background-image:url(windowsilk_bg.webp);background-repeat:repeat-y;background-position:50%;background-size:100%;font-family:'"barlow condensed"',sans-serif;font-size:22px;background-color:#dba05d;color:#101010}@media screen and (max-width:720px){body{background-image:none}}h1,h2,h3,h4,h5,h6{font-family:griffy,cursive}.heading-image{max-width:920px;width:100%;margin:auto}@media screen and (min-width:600px){.game-header{margin-top:-15px}}h1,h2{text-align:center;font-size:52px;margin-bottom:-10px;margin-top:60px}.screenshot-gallery-container h2{margin-top:50px}.buy-link-container{margin-top:-3vw}.buy-link-container .buy-link{font-family:griffy;font-size:36px;border-radius:0;filter:none;box-shadow:none;border-radius:5px;background-color:#a31119;color:#fff}.buy-link-container .buy-link:hover{background-color:pink;color:#a31119}a{color:#082749}a:hover{color:#134479}