body{margin:0;padding:0;font-family:"Mali",serif;background-color:#4b2142;color:#fff;font-size:16pt}h1,h2,h3,h4,h5,h6{font-family:"Niconne",cursive;text-align:center}#ingredient-clicker,#current-cauldron{padding:.5em}#ingredient-clicker{margin-top:.5em;display:grid;grid-template-columns:repeat(auto-fit, minmax(124px, 1fr));grid-column-gap:10px;grid-row-gap:10px;width:100%;box-sizing:border-box}#current-cauldron .oneIngredient{width:100%;background-color:rgba(255,255,255,.8);color:#000;margin-top:10px;margin-bottom:10px;box-sizing:border-box;padding:10px;border-radius:5px;display:flex;align-content:center;justify-content:space-between;align-items:center;scroll-margin:20px}#potion-result{text-align:center;background-color:rgba(0,0,0,.3);padding:2em;border-radius:.25em}#event{background-color:rgba(0,0,0,.3);padding:.5em;margin-bottom:.5em;border-radius:.25em;font-size:.66em}#event>div{margin:auto;max-width:400px}#event h2{margin-top:.33em}.winMessage{color:#64ff64;font-size:32px}#effects-in-play,#potionResult,#use-potion-button,#current-cauldron,#ingredient-clicker,#effect-explanations{display:none;-webkit-touch-callout:none;-webkit-user-select:none;-khtml-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}#effects-in-play,#effect-explanations{margin:1em}#effect-explanations{border-left:10px solid #fff;padding-left:10px;box-sizing:border-box;background-color:rgba(0,0,0,.3);padding:10px;border-radius:5px;font-size:.66em;margin-top:-1.25em}.effectName{padding:5px;background-color:rgba(0,0,0,.3);margin:5px;cursor:pointer;display:inline-block}.ingSprite,.ingSprite-small{background:url(ingredient_spritesheet.webp) no-repeat;width:120px;height:120px;background-size:cover;margin:4px;display:inline-block;image-rendering:pixelated}.ingSprite-small{width:60px;height:60px}.ingButton{background-color:rgba(255,255,255,.8);border:none;border-radius:2px;box-shadow:0 0 10px #000;padding:0;text-align:center}#solution-revealer{text-align:center;display:none;margin-top:4em;padding:1em}a.solution-btn,a.solution-btn:visited{box-sizing:border-box;display:block;background-color:#f33;color:#fff;box-shadow:0 0 6px red;font-size:16px;padding:10px;border-radius:2px;width:100%;margin:0px;cursor:pointer;width:100%;transition:color .3s,background-color .3s}a.solution-btn:hover{background-color:#4c0404;color:#ffcfcf;box-shadow:0 0 10px darkred}a.reload-button{text-decoration:underline;cursor:pointer}select,input,button{padding:5px}input,select,button{font-family:"Mali",serif;font-size:2rem;cursor:pointer;background:#7d4b72;border-radius:.5rem;color:#fff;border:none}#use-potion-button{background-color:#74226c;color:#f5bbff;box-shadow:0 0 10px purple;font-family:"Niconne",cursive;padding:1em;margin:.5em;font-size:1.45em;transition:color .3s,background-color .3s}#use-potion-button:hover{background-color:#f5bbff;color:#74226c;cursor:pointer}#event-minified{background:#c73bc7;margin:.5em;text-align:center;border-radius:.5em;cursor:pointer}.event-notification{background:red;box-shadow:0 0 0 0 #000;transform:scale(1);animation:pulse 1s infinite}@keyframes pulse{0%{transform:scale(0.95);box-shadow:0 0 0 0 rgba(0,0,0,.7)}70%{transform:scale(1);box-shadow:0 0 0 10px rgba(0,0,0,0)}100%{transform:scale(0.95);box-shadow:0 0 0 0 rgba(0,0,0,0)}}@media all and (max-width: 600px){.ingredientName-inCauldron{display:none}}

