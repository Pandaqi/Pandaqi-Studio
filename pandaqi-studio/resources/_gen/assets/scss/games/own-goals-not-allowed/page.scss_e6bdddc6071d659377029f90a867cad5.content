@font-face{font-family:'"rubik mono one"';src:url(fonts/RubikMonoOne-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:rubik;src:url(fonts/Rubik-Italic.woff2)format("woff2");font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:rubik;src:url(fonts/Rubik-Black.woff2)format("woff2");font-weight:900;font-style:normal;font-display:swap}@font-face{font-family:rubik;src:url(fonts/Rubik-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}body{font-family:rubik,serif;font-size:18px;line-height:140%;background-color:#e4fba3;background-image:url(bg_pattern.webp);color:#101010}h1,h2,h3,h4,h5,h6{font-family:'"rubik mono one"',cursive;text-align:center;font-size:40px;margin-bottom:-10px}section .center,.screenshot-gallery-container{background-color:#ffdca3;padding:10px;margin-bottom:20px;border-radius:.5em}.screenshot-gallery-container{max-width:720px;margin-left:auto;margin-right:auto}.buy-link-container .buy-link{font-family:'"rubik mono one"';font-size:36px;border-radius:0;box-shadow:none;border-radius:12px;padding-top:30px;padding-bottom:30px;background-color:#1890d5;color:#093a57;transition:color .3s,filter .3s,transform .3s;filter:drop-shadow(0 0 5px #000000)}.buy-link-container .buy-link:hover{background-color:#093a57;color:#1890d5;transform:scale(1.1)}a{color:#093a57;transition:color .3s}a:hover{color:#d71f1f}