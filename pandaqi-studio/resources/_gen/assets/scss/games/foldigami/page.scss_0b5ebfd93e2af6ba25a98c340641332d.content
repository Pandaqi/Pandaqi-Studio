@font-face{font-family:jockey;src:url(fonts/JockeyOne-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:figtree;src:url(fonts/Figtree-Black.woff2)format("woff2");font-weight:900;font-style:normal;font-display:swap}@font-face{font-family:figtree;src:url(fonts/Figtree-Italic.woff2)format("woff2");font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:figtree;src:url(fonts/Figtree-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}h1,h2,h3,h4,h5,h6,a.btn,#btn-create-pdf,#btn-generate-board,#btn-start-game,input,button,select,option,label{font-family:jockey,serif}body{font-family:figtree,sans-serif;background-color:#18393e;color:#fcfcfc;font-size:20px;background-size:100%;background-image:url(foldigami_background.webp)}@media all and (max-width:920px){body{background-image:none}}.remark-under-settings{font-size:.75em;opacity:.75}.divider-image{max-width:920px;margin:auto;margin-top:3em}.limit-width{max-width:920px}h2{font-size:56px;margin-bottom:0}h3{font-size:38px;margin-bottom:0}a{color:#b2da55;transition:color .3s}a:hover{color:#d8f398}.game-settings{background-color:#a51921}.boardgame-intro .tagline,.boardgame-intro .metadata{color:#fcfcfc}.boardgame-intro .tagline{font-size:1.25em}.boardgame-intro .metadata{font-size:.85em}.boardgame-intro .big-header-image{filter:none!important}a.download-btn,a.download-btn:visited,#btn-start-game,#btn-start-game:visited,#btn-generate-board,#btn-generate-board:visited{background-color:#fcc038;color:#212121}a.download-btn:hover,a.download-btn:visited:hover,#btn-start-game:hover,#btn-start-game:visited:hover,#btn-generate-board:hover,#btn-generate-board:visited:hover{background-color:#212121;color:#fcc038}