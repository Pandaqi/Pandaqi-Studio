@font-face{font-family:hanalei fill;src:url(/square-ogre/assets/fonts/HanaleiFill-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:andika;src:url(/square-ogre/assets/fonts/Andika-Italic.woff2)format("woff2");font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:andika;src:url(/square-ogre/assets/fonts/Andika.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:andika;src:url(/square-ogre/assets/fonts/Andika-Bold.woff2)format("woff2");font-weight:700;font-style:normal;font-display:swap}body{background-image:url(squareogre_bg.webp);background-repeat:repeat-y;background-position:50%;background-color:#070110;background-size:100%;font-family:andika,sans-serif;font-size:20px;color:#fcfcfc}h1,h2,h3,h4,h5,h6{font-family:hanalei fill,cursive;font-weight:400}.limit-width,.image-as-heading img,section .center{max-width:900px;margin:auto}img[src$="#float"]{max-width:300px;float:left}.image-as-heading{max-width:100%;margin-top:40px;margin-bottom:-50px;text-align:center}.image-as-heading img{width:100%}@media all and (max-width:660px){.image-as-heading{margin-bottom:-30px}}h1{text-align:center;font-size:52px;margin-bottom:10px;margin-top:40px;color:#fcfcfc}.buy-link,main a{padding:20px;background-color:#6bb73e;color:rgba(0,0,0,.75);font-family:hanalei fill,cursive;font-size:32px;margin:10px;max-width:100%}.buy-link:hover,main a:hover{background-color:#944fc0;color:rgba(255,255,255,.75)}main a{display:block;margin:auto;margin-top:20px;width:auto;max-width:400px;font-size:24px;border-radius:.5em;text-decoration:none}