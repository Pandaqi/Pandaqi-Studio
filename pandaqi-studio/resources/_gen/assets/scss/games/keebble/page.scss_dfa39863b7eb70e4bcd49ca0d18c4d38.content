@font-face{font-family:arbutus slab;src:url(/keebble/assets/ArbutusSlab-Regular.woff2);font-weight:400;font-style:normal;font-display:swap}h1,h2,h3,h4,h5,h6,a.btn,#btn-create-pdf,#btn-generate-board,#btn-start-game,input,button,select,option,label{font-family:arbutus slab,sans-serif}body{font-family:arbutus slab,serif;background-color:#d8d8d8;font-size:20px;background-size:100%}label{font-size:18px}.limit-width{max-width:960px}h2{font-size:56px;margin-bottom:0}h3{font-size:38px;margin-bottom:0}a{color:#7c2b6b;transition:color .3s}a:hover{color:#426613}.game-settings{background-color:#7c2b6b}a.download-btn,a.download-btn:visited,#btn-start-game,#btn-start-game:visited,#btn-generate-board,#btn-generate-board:visited{background-color:#7c2b6b;color:#fff}a.download-btn:hover,a.download-btn:visited:hover,#btn-start-game:hover,#btn-start-game:visited:hover,#btn-generate-board:hover,#btn-generate-board:visited:hover{background-color:#93d33e;color:#111}@media all and (max-width:900px){body{background-image:none}.tagline{background-color:#9ebc8b;padding:1em;border-radius:1em}.game-settings{font-size:12px}}