@font-face{font-family:woodcutter;src:url(/epic-medics/css/woodcutter.woff2);font-weight:400;font-style:normal;font-display:swap}body{font-family:pt serif,serif;background-color:#d5deff;color:#111;font-size:24px}select{width:50px;height:50px;font-family:woodcutter;background-color:#123254;border:none;color:#fff;border-radius:7px}.event{padding:20px;box-sizing:border-box;background-color:rgba(0,0,0,.7);color:#fff;border-radius:10px;box-shadow:0 0 10px #000;position:relative}.event .takeAction{display:block;margin:20px;border-left:10px solid #fff;margin-left:0;padding-left:10px;font-weight:700}.event .roundNumber{position:absolute;right:10px;top:10px;font-weight:700;color:rgba(255,255,255,.3)}#curEventDisplay{max-width:600px;margin:auto;margin-bottom:1.5em}a.btn,a.btn:visited,a.download-btn,a.download-btn:visited,a.event-btn,a.event-btn:visited{font-family:woodcutter;background-color:#94a6f4;color:#000;border-radius:10px;padding:1em;cursor:pointer}a.btn:hover,a.download-btn:hover,a.event-btn:hover{background-color:#000064;color:#afafff}