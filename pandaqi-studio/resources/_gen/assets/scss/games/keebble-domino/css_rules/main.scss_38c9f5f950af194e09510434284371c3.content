ge:url(/keebble-domino/assets/special_cells.webp);background-size:800%;background-position-x:0%;background-position-y:-100%;margin:auto}.icon-wall{background-image:url(/keebble-domino/assets/special_cells.webp);background-size:800%;background-position-x:-100%;background-position-y:-100%;margin:auto}.icon-x-ray{background-image:url(/keebble-domino/assets/special_cells.webp);background-size:800%;background-position-x:-200%;background-position-y:-100%;margin:auto}h1,h2,h3,h4,h5,h6{font-family:"Arbutus Slab",sans-serif}body{font-family:"Arbutus Slab",serif;background-color:#fcfcfc;font-size:17px}h1{font-size:3.5em}h2{font-size:2em;string-set:title content(text)}h3{font-size:1em}ul{margin-top:-0.85em;margin-bottom:1.5em}#setup{background-color:#d9ffa9}#objective{background-color:#68fed3}#expansions{background-color:#bebef6}#action_play{background-color:#ffb6df}#action_scoring{background-color:#ffd68f}#how_it_works{background-color:#ffcbcb}@media print{body{font-size:14px}#setup{background-color:rgba(0,0,0,0) !important}#setup .heading-container{background-color:#d9ffa9}#objective{background-color:rgba(0,0,0,0) !important}#objective .heading-container{background-color:#68fed3}#expansions{background-color:rgba(0,0,0,0) !important}#expansions .heading-container{background-color:#bebef6}#action_play{background-color:rgba(0,0,0,0) !important}#action_play .heading-container{background-color:#ffb6df}#action_scoring{background-color:rgba(0,0,0,0) !important}#action_scoring .heading-container{background-color:#ffd68f}#how_it_works{background-color:rgba(0,0,0,0) !important}#how_it_works .heading-container{background-color:#ffcbcb}}