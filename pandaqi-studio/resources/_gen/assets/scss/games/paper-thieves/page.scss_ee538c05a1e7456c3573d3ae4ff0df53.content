@font-face{font-family:'"baloo 2"';src:url(fonts/Baloo2-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:'"baloo 2"';src:url(fonts/Baloo2-ExtraBold.woff2)format("woff2");font-weight:700;font-style:normal;font-display:swap}body{font-family:'"baloo 2"',cursive;background-color:#1a1d33;color:#fff;line-height:120%;font-size:20px}h1,h2,h3,h4,h5,h6{font-family:'"baloo 2"',cursive;line-height:120%}h2{font-size:48px;margin-bottom:0}.board-image{max-width:960px;margin:auto;padding:20px}.boardgame-intro .tagline,.boardgame-intro .metadata{color:#fff}a{color:#d3ceff;transition:color .3s}a:hover{color:#fb3640}a.download-btn,a.download-btn:visited{background-color:#fb3640;color:#fff;line-height:120%;font-family:"baloo 2",cursive}a.download-btn:hover{background-color:#ff6a64;color:rgba(0,0,0,.75)}