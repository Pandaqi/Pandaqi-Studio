@font-face{font-family:sketchy;src:url(sketchy.woff2)}h1,h2,h3,h4,h5,h6{font-family:sketchy}body{background-repeat:repeat;font-family:sriracha,Calibri,Georgia,cursive;font-size:20px;line-height:120%;background-color:#fbf0e2;color:#222;position:relative;height:100%}.bgDiv{background-image:url(stars.webp);filter:invert();position:absolute;height:100%;width:100%;top:0;left:0;z-index:-50;opacity:.05}#main{max-width:550px;margin:auto;padding:10px;box-sizing:border-box}h2{text-decoration:none;margin-bottom:0;margin-top:60px;font-size:54px}#playStoreDownloadButton{display:block;margin:auto;max-width:320px;height:80px;background:url(downloadButton.webp);background-size:100%}#playStoreDownloadButton:hover,#playStoreDownloadButton:active{background:url(downloadButtonOver.webp);background-size:100%}.art-hockey-red{color:#640000}.art-hockey-blue{color:#000064}.art-hockey-green{color:#006400}.art-hockey-purple{color:#640064}@media all and (max-width:600px){h2{font-size:8vw}}