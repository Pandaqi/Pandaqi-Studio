h1,h2,h3,h4,h5,h6{font-family:'Patua One', cursive;text-decoration:none;text-transform:none;text-align:center;margin-top:0px;margin-bottom:0px}h1{font-size:max(6vw,48px)}h2{font-size:48px}h3{margin-top:0px}body{font-family:'Poppins', sans-serif;font-size:16pt;background-color:#fad8d6}@media all and (max-width: 660px){body{font-size:12pt}}section .center{max-width:960px}.game-grid{display:grid;grid-template-columns:repeat(auto-fit, minmax(300px, 1fr));grid-column-gap:10px;grid-row-gap:10px;width:100%;box-sizing:border-box;padding:20px}.game-grid a{padding:20px 20px;border-radius:10px;box-shadow:inset 0 -4px 0 rgba(0,0,0,0.4);border-width:0;text-decoration:none;box-sizing:border-box;background-color:rgba(255,255,255,0.4);color:black;display:block;transition:background 0.3s, color 0.3s}.game-grid a:hover{background-color:rgba(0,0,0,0.5);color:white}.game-grid a img{filter:drop-shadow(0 0 5px #333);max-height:initial !important}ul,ol{margin-top:-20px}p.fullWidthParagraph{max-width:960px;text-align:center}.underline-image img{border-bottom:5px solid rgba(0,0,0,0.2);border-top:5px solid rgba(0,0,0,0.7)}.background-pattern{background-image:url(webp/boardgames_pattern.webp);background-size:contain;padding-bottom:30px}@media all and (max-width: 600px){.background-pattern{background-image:none}}.btn{padding:10px 20px;border-radius:15px;box-shadow:inset 0 -4px 0 rgba(0,0,0,0.4);border-width:0;text-decoration:none;font-weight:bold;background:rgba(255,255,255,0.9);color:black;transition:background 0.3s, color 0.3s;display:inline-block;margin:10px}.btn:hover{background:rgba(0,0,0,0.1);color:white}.full-category-overview-link{color:rgba(0,0,0,0.75);text-align:center;display:block;transition:color 0.3s, background-color 0.3s}.full-category-overview-link:hover{color:rgba(255,255,255,0.75);background-color:rgba(0,0,0,0.5)}.colorScheme-red{background-color:#9a0901;border-color:#cd1c14;color:white}.colorScheme-beige{background-color:#edb88b;border-color:#cb9669}.colorScheme-green{background-color:#55d855;border-color:#022e02}.colorScheme-blue{background-color:#39dfdd;border-color:#17bebb}.colorScheme-black{background-color:#2e282a;border-color:#4f4a4c;color:#E8E8E8}

