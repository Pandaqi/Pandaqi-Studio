@font-face{font-family:proxima nova rg;src:url(fonts/proxnov-bold.woff2);font-weight:700;font-style:normal;font-display:swap}@font-face{font-family:proxima nova rg;src:url(fonts/proxnov-it.woff2);font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:proxima nova rg;src:url(fonts/proxnov-reg.woff2);font-weight:400;font-style:normal;font-display:swap}.redCategory{background-color:#e03926;color:#fff}.blueCategory{background-color:#26526d;color:#fff}.purpleCategory{background-color:#59365e;color:#fff}.greenCategory{background-color:#53672e;color:#fff}h2{font-size:64px;margin-bottom:0;box-sizing:border-box;padding:20px;text-align:center;box-shadow:inset 0 -10px rgba(0,0,0,.4);border-width:0}h1,h2,h3,h4,h5,h6{font-family:proxima nova rg,cursive}body{font-family:proxima nova rg,cursive;font-size:16pt;line-height:130%;background-color:#d2e6ab;color:#000}a,a:visited{color:#a9123d}a.btn,a.btn:visited{font-family:proxima nova rg;background-color:#8d2f28;color:#fff;padding:40px}a.btn:hover{background-color:#e03926;color:#fff}.big-header-image{filter:none!important}.big-header-image:hover{filter:drop-shadow(0 0 10px black)}@media all and (max-width:600px){h2{font-size:32px}}