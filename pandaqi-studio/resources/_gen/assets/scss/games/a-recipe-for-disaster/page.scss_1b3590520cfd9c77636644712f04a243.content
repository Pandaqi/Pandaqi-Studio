@font-face{font-family:chewy;src:url(fonts/Chewy-Regular.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:'"open sans condensed"';src:url(fonts/OpenSans-CondensedBold.woff2)format("woff2");font-weight:700;font-style:normal;font-display:swap}body{background-image:url(arfd_pattern_full.webp);background-repeat:repeat-y;background-position:50%;background-size:100%;font-family:'"open sans condensed"',sans-serif;font-size:20px;color:#101010}h1,h2,h3,h4,h5,h6{font-family:chewy,cursive}h1,h2{text-align:center;font-size:52px;margin-bottom:10px;margin-top:40px}.buy-link,a{font-family:chewy;font-size:36px;display:block;max-width:500px;margin:auto;padding:20px;background-color:#6bb73e;color:#1b2e0f;text-align:center;text-decoration:none;border-radius:10px}.buy-link:hover,a:hover{background-color:#944fc0;color:rgba(255,255,255,.75)}@media all and (max-width:500px){.buy-link,a{font-size:20px}}