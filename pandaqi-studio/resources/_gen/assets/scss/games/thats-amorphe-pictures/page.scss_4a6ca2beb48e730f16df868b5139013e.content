h1,h2,h3,h4,h5,h6,a.btn,#btn-create-pdf,#btn-generate-board,#btn-start-game{font-family:ribeye,sans-serif}body{font-family:lora,serif;background-color:#b4f9ff;font-size:20px}input,button,select,option,label{font-family:lora,serif}label{font-size:18px}.limit-width{max-width:960px}h2{font-size:56px;margin-bottom:0}h3{font-size:38px;margin-bottom:-18px}h3.settings-heading{margin-bottom:0;color:rgba(255,255,255,.87)}a{color:#0b3a3e;transition:color .3s}a:hover{color:#3d0911}.boardgame-intro .big-header-image{filter:none}.game-settings{background-color:#0b3a3e;color:#eef}a.download-btn,a.download-btn:visited,#btn-start-game,#btn-start-game:visited{background-color:#0b3a3e;color:#eef}a.download-btn:hover,#btn-start-game:hover{background-color:#f64b64;color:#3d0911;box-shadow:0 0 10px #333}@media all and (max-width:900px){.game-settings{font-size:12px}}