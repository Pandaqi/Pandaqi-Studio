body{margin:0;padding:0;font-family:"Chelsea Market",sans-serif;font-size:20px;background-color:#eeeeab}#phaser-container{margin:0;padding:0;display:none}#interface-container,#game-images-container{position:absolute;left:0;right:0;top:0;bottom:0;display:flex}#interface-container{filter:drop-shadow(0px 0px 5px black)}.interface-container-lowvis{filter:none !important}#interface{margin:auto;background-color:#fff;width:500px;padding:14px;border-radius:10px;border:5px solid #000}.interface-lowvis{width:100px !important;background:rgba(0,0,0,0) !important;border:none !important;opacity:.4}button,select{width:100%;font-family:"Chelsea Market",sans-serif;font-size:24px;background:#ff7600;border-radius:5px;border:none;transition:background .3s,color .3s;margin-top:5px;margin-bottom:5px}button:hover,select:hover{background:#f22;cursor:pointer;color:#fff}#game-images-container{justify-content:center;align-items:center;align-content:center}#game-images-container img{max-width:100%;max-height:100%}#hint-images-container{display:flex;justify-content:center;align-items:center;align-content:center;flex-wrap:wrap}#hint-images-list{display:flex;justify-content:center;align-content:center;flex-wrap:wrap}#hint-image{width:192px;height:192px}#hint-image img{width:100%;height:100%}.dropdown-container{display:flex;justify-content:center;align-content:center;align-items:center;gap:20px;grid-gap:20px;flex-gap:20px}.interface-reload-message{font-size:12px;opacity:.5;margin-top:-20px}#debugging-container{position:absolute;left:0;right:0;top:0;bottom:0;display:flex;justify-content:center}#debugging{display:flex;height:96px;justify-content:center;align-content:center;gap:10px}