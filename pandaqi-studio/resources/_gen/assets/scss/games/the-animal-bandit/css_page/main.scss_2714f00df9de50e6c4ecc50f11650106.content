@font-face{font-family:"Kalyant Demo";src:url("fonts/kalyant.woff2") format("woff2");font-weight:300;font-style:normal;font-display:swap}@font-face{font-family:"Berlin Sans FB Demi";src:url("fonts/berlinsans.woff2") format("woff2");font-weight:bold;font-style:normal;font-display:swap}h1,h2,h3,h4,h5,h6{font-family:"Berlin Sans FB Demi",cursive;text-align:center}h2{margin-top:60px;margin-bottom:-15px;font-size:max(5vw,28px)}h3{margin-top:40px;margin-bottom:-15px;font-size:max(2.5vw,20px)}body{font-family:"Kalyant Demo",cursive;background-color:#f2d64a;color:#3a1022;font-size:24px}.board-image{text-align:center}a.btn,a.btn:visited{background-color:#536acc;color:#000}a.btn:hover{background-color:#000064;color:#afafff}a.download-btn,a.download-btn:visited{font-family:"Berlin Sans FB Demi";background-color:#ff2900;color:#ffc4aa}a.download-btn:hover{background-color:#ffc4aa;color:#ff2900}ul{margin-top:-20px}.boardgame-intro .big-header-image{position:absolute;left:0;right:0;top:0;bottom:0;opacity:.2;z-index:-1;max-width:100%;width:100%}.boardgame-intro .text{margin-top:12vw}.boardgame-intro .text h1{font-size:7vw;text-align:center;margin-bottom:0px;margin-top:0px}