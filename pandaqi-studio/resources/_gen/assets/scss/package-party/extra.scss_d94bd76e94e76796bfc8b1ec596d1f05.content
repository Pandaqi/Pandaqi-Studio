body{background-image:url("webp/bg_pattern.webp");background-repeat:repeat;font-family:"Open Sans";font-size:20px}h1,h2,h3,h4,h5,h6{font-family:"Fredoka One"}h2{font-size:42px;margin-bottom:0px;margin-top:0px;text-decoration:1pt wavy;text-transform:uppercase;text-decoration-skip-ink:none;text-decoration-line:underline;color:#300}.buy-link{margin-top:60px;display:inline-block;padding:20px;background-color:brown;color:#fff;border-radius:.5em;text-decoration:none;font-family:"Fredoka One";transition:background-color .5s,color .5s;box-shadow:0 0 10px 2px #333}.buy-link:hover{background-color:#fff;color:#333}.limit-width{max-width:1280px}.game-header .video-no-header{max-width:900px}section a{display:block;padding:20px;background-color:#0e6a43;color:#fff;border-radius:.5em;text-decoration:none;font-family:"Fredoka One";transition:background-color .5s,color .5s;box-shadow:0 0 10px 2px #333}section a:hover{background-color:#fff;color:#333}