h1,h2,h3,h4,h5,h6,a.btn,#btn-create-pdf,#btn-generate-board,#btn-start-game{font-family:"Ribeye",sans-serif}body{font-family:"Lora",serif;background-color:#feb3b3;font-size:20px}input,button,select,option,label{font-family:"Lora",serif}label{font-size:18px}.limit-width{max-width:960px}h2{font-size:56px;margin-bottom:0px}h3{font-size:38px;margin-bottom:-18px}h3.settings-heading{margin-bottom:0px;color:rgba(255,255,255,.87)}a{color:#8d1919;transition:color .3s}a:hover{color:#0a3a3e}.boardgame-intro .big-header-image{filter:none}.game-settings{background-color:#8d1919;color:#fee}a.download-btn,a.download-btn:visited,#btn-start-game,#btn-start-game:visited{background-color:#8d1919;color:#fee}a.download-btn:hover,#btn-start-game:hover{background-color:#aacae7;color:#0a3a3e;box-shadow:0 0 10px #333}@media all and (max-width: 900px){.game-settings{font-size:12px}}