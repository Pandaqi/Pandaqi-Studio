@font-face{font-family:"SciFly";src:url("fonts/scifly.woff2");font-weight:normal;font-style:normal;font-display:swap}h1,h2,h3,h4,h5,h6{font-family:"SciFly"}body{font-family:"SciFly";background:url(webp/unstableuniverse_bg.webp),linear-gradient(to right, rgb(126, 166, 204) 0%, rgb(194, 221, 247) 3%, rgb(194, 221, 247) 97%, rgb(126, 166, 204) 100%);background-size:100%;font-size:22px}.review-container{padding:20px;max-width:1280px;margin:auto}.boardgame-intro .big-header-image{filter:none}.boardgame-intro .big-header-image img{border-bottom:5px solid #3d6de3}.boardgame-intro .text{margin-top:-10vw;background-color:#c2ddf7;position:relative;border-top:5px solid #3d6de3;border-radius:.5em}strong{color:#9e2d41}.post-settings-remarks{font-size:16px;opacity:.66}h2{font-size:60px;margin-bottom:-15px}h3{font-size:38px;margin-bottom:-18px}a.link{color:#248232;border-bottom:2px solid;text-decoration:none;transition:color .3s,background-color .3s}a.link:hover{color:#e33d3d}a.btn,a.btn:visited,button.btn{font-family:"SciFly"}.game-settings{background-color:#154558}#btn-generate-board{font-family:"SciFly"}#btn-create-pdf{font-weight:900;font-family:"SciFly",sans-serif;font-size:24pt;background-color:#f66;color:#fcfffc;transition:background-color .3s,color .3s}#btn-create-pdf:hover{background-color:#faa;color:#111}a.download-btn,a.download-btn:visited{background-color:#154558;color:#fff}a.download-btn:hover{background-color:#feab97;color:#000}@media all and (max-width: 900px){body{background:none;background-color:#c2ddf7}}